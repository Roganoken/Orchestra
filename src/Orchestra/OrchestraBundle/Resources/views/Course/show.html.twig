<h1>Course</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Fullname</th>
            <td>{{ entity.fullName }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Exam</th>
            <td>{{ entity.exam }}</td>
        </tr>
        <tr>
            <th>Program</th>
            <td>{{ entity.program }}</td>
        </tr>
        <tr>
            <th>Duration</th>
            <td>{{ entity.duration }}</td>
        </tr>
        <tr>
            <th>Start</th>
            <td>{{ entity.start|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>End</th>
            <td>{{ entity.end|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Coefficient</th>
            <td>{{ entity.coefficient }}</td>
        </tr>
        <tr>
            <th>Isqualifying</th>
            <td>{{ entity.isQualifying }}</td>
        </tr>
        <tr>
            <th>Isfede</th>
            <td>{{ entity.isFede }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('course') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('course_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('course_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
