<h1>Address list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Address1</th>
            <th>Address2</th>
            <th>Address3</th>
            <th>Other</th>
            <th>Phone1</th>
            <th>Phone2</th>
            <th>Fax</th>
            <th>Isactive</th>
            <th>Isdeleted</th>
            <th>Created</th>
            <th>Updated</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('address_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.address1 }}</td>
            <td>{{ entity.address2 }}</td>
            <td>{{ entity.address3 }}</td>
            <td>{{ entity.other }}</td>
            <td>{{ entity.phone1 }}</td>
            <td>{{ entity.phone2 }}</td>
            <td>{{ entity.fax }}</td>
            <td>{{ entity.isActive }}</td>
            <td>{{ entity.isDeleted }}</td>
            <td>{% if entity.created %}{{ entity.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.updated %}{{ entity.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('address_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('address_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('address_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
