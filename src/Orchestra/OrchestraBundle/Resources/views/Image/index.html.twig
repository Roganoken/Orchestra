<h1>Image list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Titre</th>
            <th>Descriptif</th>
            <th>Datecreation</th>
            <th>Url</th>
            <th>Taille</th>
            <th>Poids</th>
            <th>Logiciel</th>
            <th>Motcle</th>
            <th>Created</th>
            <th>Updated</th>
            <th>Isportfolio</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('image_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.titre }}</td>
            <td>{{ entity.descriptif }}</td>
            <td>{% if entity.dateCreation %}{{ entity.dateCreation|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.url }}</td>
            <td>{{ entity.taille }}</td>
            <td>{{ entity.poids }}</td>
            <td>{{ entity.logiciel }}</td>
            <td>{{ entity.motCle }}</td>
            <td>{{ entity.created }}</td>
            <td>{{ entity.updated }}</td>
            <td>{{ entity.isPortfolio }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('image_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('image_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('image_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
