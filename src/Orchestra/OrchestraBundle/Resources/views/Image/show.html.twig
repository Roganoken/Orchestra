<h1>Image</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Titre</th>
            <td>{{ entity.titre }}</td>
        </tr>
        <tr>
            <th>Descriptif</th>
            <td>{{ entity.descriptif }}</td>
        </tr>
        <tr>
            <th>Datecreation</th>
            <td>{{ entity.dateCreation|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Url</th>
            <td>{{ entity.url }}</td>
        </tr>
        <tr>
            <th>Taille</th>
            <td>{{ entity.taille }}</td>
        </tr>
        <tr>
            <th>Poids</th>
            <td>{{ entity.poids }}</td>
        </tr>
        <tr>
            <th>Logiciel</th>
            <td>{{ entity.logiciel }}</td>
        </tr>
        <tr>
            <th>Motcle</th>
            <td>{{ entity.motCle }}</td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ entity.created }}</td>
        </tr>
        <tr>
            <th>Updated</th>
            <td>{{ entity.updated }}</td>
        </tr>
        <tr>
            <th>Isportfolio</th>
            <td>{{ entity.isPortfolio }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('image') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('image_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('image_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
