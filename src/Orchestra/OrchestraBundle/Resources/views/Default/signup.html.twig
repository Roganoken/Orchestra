{% extends '::base.html.twig' %}

{% block title %}
Bienvenue !
{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bundles/orchestraorchestra/css/style_connexion.css') }}" type="text/css" media="all" />
{% endblock %}

{% block header %}
<div id="logo" >
    <img class="image_left" src="{{ asset ('bundles/orchestraorchestra/img/estei.png') }}" alt="" />
    <p id="txt_logo">Ecole supérieure des technologies électronique, informatique et infographie</p>
</div>
         
{% endblock %}

{% block contenu %}

<div id="contenu">

    <div id="bloc_connexion">
        <p id="txt_connexion">S'enregistrer</p>
        <button id="esteiens" type="submit" onclick="return confirm('Etes-vous sûr de vouloir modifier ?')">Vous êtes un(e) Esteien(ne)</button>
        <button id="pros" type="submit" onclick="return confirm('Etes-vous sûr de vouloir modifier ?')">Vous êtes un Professionnel(le)</button>
        
    </div>
    <div id="pictos">    
        <div id="picto_connex"><a href="{{ path('fos_user_security_login') }}"><img src="{{ asset ('bundles/orchestraorchestra/img/picto_vide.png') }}" alt="picto connexion" /></a></div>
        <div id="picto_inscrip" class="active"><img src="{{ asset ('bundles/orchestraorchestra/img/picto_vide.png') }}" alt="picto inscription" /></div>
    </div>



</div>

{% endblock %}
