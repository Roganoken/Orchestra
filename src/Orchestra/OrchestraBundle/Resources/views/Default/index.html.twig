{% extends '::base.html.twig' %}

{% block title %}
Bienvenue !
{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bundles/orchestraorchestra/css/style_connexion.css') }}" type="text/css" media="all" />
{% endblock %}

{% block header %}
<div id="logo" >
    <img class="image_left" src="{{ asset ('bundles/orchestraorchestra/img/estei.png') }}" alt="" />
    <p id="txt_logo">Ecole supérieure des technologies électronique, informatique et infographie</p>
</div>
         
{% endblock %}

{% block contenu %}

<div id="contenu">

    <div id="bloc_connexion">
        <p id="txt_connexion">Se connecter</p>

        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
        
    </div>
    <div id="pictos">    
        <div id="picto_connex" class="active"><img src="{{ asset ('bundles/orchestraorchestra/img/picto_vide.png') }}" alt="picto inscription" /></div>
        <div id="picto_inscrip"><a href="{{ path('signup') }}"><img src="{{ asset ('bundles/orchestraorchestra/img/picto_vide.png') }}" alt="picto inscription" /></a></div>
    </div>



</div>

{% endblock %}
